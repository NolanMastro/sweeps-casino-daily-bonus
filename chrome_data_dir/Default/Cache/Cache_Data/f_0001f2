eof cb !== 'function') {\n        return undefined;\n      }\n\n      this.listeners[eventType] = this.listeners[eventType] || [];\n      this.listeners[eventType].push(cb);\n      return undefined;\n    },\n    removeListener(eventType: KustomerWidgetEventTypes, cb) {\n      if (!this.listeners[eventType] || typeof cb !== 'function') {\n        return;\n      }\n      this.listeners[eventType] = this.listeners[eventType].filter(\n        (callback) => callback !== cb,\n      );\n    },\n    stop(callback) {\n      const iframe = getIframeForWidgetMain();\n      iframe?.remove();\n      this.startFinished = false;\n      if (typeof callback === 'function') {\n        callback();\n      }\n    },\n    start(parameters: StartParameters, cb) {\n      this.startParameters = parseStartParameters(parameters);\n      this.startCallback = cb;\n      if (getIframeForWidgetMain()) {\n        return;\n      }\n      setKustomerStyleSheetOnParent();\n\n      const zIndex = this.startParameters.zIndex || DEFAULT_Z_INDEX;\n      const position = this.startParameters.chatIconPosition;\n      const horizontalPadding =\n        position.alignment === 'right'\n          ? `right: ${Number(position.horizontalPadding)}px`\n          : `left: ${Number(position.horizontalPadding)}px`;\n\n      // Get nonce from script element, similar to widget_queue\n      const nonce = this.getNonce();\n\n      setupIframeForApi(\n        'kustomer-ui-sdk-iframe',\n        `display: none; border: 0; width: 0px; height: 0px; position: fixed; bottom: ${Number(\n          position.verticalPadding,\n        )}px; ${horizontalPadding}; z-index: ${Number(zIndex)};`,\n        buildSrcdoc({\n          cssReset,\n          chatIconAlign: position.alignment,\n          coreSrc: `${process.env.CHAT_WEB_BASE_URL}/core.js`,\n          widgetSrc: process.env.KUSTOMER_WIDGET_MAIN_INCLUDE!,\n          apiKey: kustomer.a,\n          shop: kustomer.b,\n          nonce,\n        }),\n        'Kustomer Widget Iframe',\n        nonce,\n      );\n    },\n    open(callback) {\n      const iframe = getIframeForWidgetMain();\n      if (iframe && iframe.contentWindow) {\n        const event = new CustomEvent('kustomerOpen', {\n          detail: { callback },\n        });\n        iframe.contentWindow.dispatchEvent(event);\n      }\n    },\n    close(callback) {\n      const iframe = getIframeForWidgetMain();\n      if (iframe && iframe.contentWindow) {\n        const event = new CustomEvent('kustomerClose', {\n          detail: { callback },\n        });\n        iframe.contentWindow.dispatchEvent(event);\n      }\n    },\n    login(parameters: LoginParameters, callback) {\n      const iframe = getIframeForWidgetMain();\n\n      if (iframe && iframe.contentWindow) {\n        const event = new CustomEvent('kustomerLogin', {\n          detail: { parameters, callback },\n        });\n        iframe.contentWindow.dispatchEvent(event);\n      }\n    },\n    isLoggedIn(parameters: IsLoggedInParameters, callback) {\n      const iframe = getIframeForWidgetMain();\n      const contentWindow = iframe?.contentWindow as KustomerWidgetWindow;\n      if (iframe && iframe.contentWindow) {\n        contentWindow.KustomerCore?.isLoggedIn(parameters, callback);\n      }\n    },\n    logout(cb) {\n      const iframe = getIframeForWidgetMain();\n      const contentWindow = iframe?.contentWindow as KustomerWidgetWindow;\n      if (iframe && contentWindow) {\n        contentWindow.KustomerCore?.logout(cb);\n      }\n    },\n    describeCustomer(parameters: DescribeCustomerParameters, cb) {\n      const iframe = getIframeForWidgetMain();\n      const contentWindow = iframe?.contentWindow as KustomerWidgetWindow;\n      if (iframe && contentWindow) {\n        contentWindow.KustomerCore?.describeCustomer(parameters, cb);\n      }\n    },\n    describeConversation(parameters: DescribeConversationParameters, cb) {\n      const iframe = getIframeForWidgetMain();\n      const contentWindow = iframe?.contentWindow as 