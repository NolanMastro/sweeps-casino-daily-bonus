;\n  }\n  if (typeof chatIconSize === 'number') {\n    return {\n      height: chatIconSize,\n      width: chatIconSize,\n    };\n  }\n  if (\n    typeof chatIconSize?.height === 'number' &&\n    typeof chatIconSize?.width === 'number'\n  ) {\n    return chatIconSize;\n  }\n  if (chatIconSize) {\n    throw Error('Unable to parse chatIconSize parameter');\n  }\n  return undefined;\n};\n\nexport const parseChatIconPosition = (\n  chatIconPosition?: string | ChatIconPosition,\n) => {\n  const position: ChatIconPosition = { ...DEFAULT_CHAT_ICON_POSITION };\n  if (chatIconPosition && typeof chatIconPosition === 'string') {\n    if (chatIconPosition === 'right' || chatIconPosition === 'left')\n      position.alignment = chatIconPosition;\n    else {\n      throw Error('Unable to parse chatIconPosition parameter');\n    }\n  }\n  if (typeof chatIconPosition === 'object') {\n    Object.assign(position, chatIconPosition);\n  }\n  if (\n    !['right', 'left'].includes(position.alignment) ||\n    typeof position.horizontalPadding !== 'number' ||\n    typeof position.verticalPadding !== 'number'\n  ) {\n    throw Error('Unable to parse chatIconPosition parameter');\n  }\n  return position;\n};\n\nexport const parseStartParameters = (\n  startParameters?: StartParameters,\n): NormalizedStartParameters => {\n  if (!startParameters)\n    return {\n      chatIconPosition: DEFAULT_CHAT_ICON_POSITION,\n    };\n  return {\n    ...startParameters,\n    chatIconSize: parseChatIconSize(startParameters?.chatIconSize),\n    chatIconPosition: parseChatIconPosition(startParameters?.chatIconPosition),\n  };\n};\n\nexport const setKustomerStyleSheetOnParent = () => {\n  const sheetExists = window.document?.getElementById('kustomer-mobile-style');\n\n  if (sheetExists) return;\n\n  const parentHead = window.document?.head;\n\n  const kustomerParentStyleSheet = window.document?.createElement('style');\n\n  kustomerParentStyleSheet.id = 'kustomer-mobile-style';\n\n  kustomerParentStyleSheet.inner